<action>
	<icon></icon>
	<name>File path</name>
	<submenu>Copy</submenu>
	<unique-id>1893637941349276-1</unique-id>
	<command>echo -n &quot;%F&quot; | xclip -selection clipboard</command>
	<description>Copy file path with quotes to clipboard</description>
	<range></range>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
